{
  "private": true,
  "version": "1.0.0-beta6",
  "author": "Robin Frischmann <robin@rofrischmann.de>",
  "license": "MIT",
  "repository": "https://github.com/rofrischmann/react-look",
  "scripts": {
    "build": "./scripts/buildPackages.sh",
    "clean": "rimraf coverage",
    "coverage": "codeclimate-test-reporter < coverage/lcov.info",
    "demo": "webpack-dev-server --config webpack.config.demo.js --no-info --content-base packages/react-look/demo/",
    "demo:universal": "concurrent --kill-others \"npm run demo\" \"babel-node packages/react-look/demo/server.js\"",
    "lint": "./scripts/lintPackages.sh",
    "prepare": "babel-node scripts/preparePackages.js",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build && npm run prepare",
    "install:core": "./scripts/installLocalCorePackage.sh",
    "release": "npm run prerelease && ./scripts/releasePackages.sh",
    "test": "istanbul cover _mocha -- packages/react-look-core/test packages/react-look/test packages/react-look-native/test --opts test/mocha.opts",
    "umd": "NODE_ENV=development webpack packages/react-look/modules/index.js packages/react-look/dist/react-look.js",
    "umd:min": "NODE_ENV=production webpack packages/react-look/modules/index.js packages/react-look/dist/react-look.min.js"
  },
  "devDependencies": {
    "assign-styles": "^2.0.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-lodash": "^2.1.0",
    "babel-plugin-module-alias": "^1.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "color": "^0.11.1",
    "concurrent": "^0.3.2",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "enzyme": "^2.1.0",
    "esformatter": "^0.9.0",
    "esformatter-braces": "^1.2.1",
    "esformatter-collapse-objects": "^0.5.1",
    "esformatter-dot-notation": "^1.3.1",
    "esformatter-jsx": "^4.1.2",
    "esformatter-literal-notation": "^1.0.1",
    "esformatter-parseint": "^1.0.3",
    "esformatter-quote-props": "^1.0.2",
    "esformatter-quotes": "^1.0.3",
    "esformatter-remove-trailing-commas": "^1.0.1",
    "esformatter-spaced-lined-comment": "^2.0.1",
    "esformatter-var-each": "^2.1.0",
    "eslint": "^2.3.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^4.1.0",
    "estraverse-fb": "^1.3.1",
    "express": "^4.13.3",
    "express-http-proxy": "^0.6.0",
    "fs-extra": "^0.26.5",
    "inline-style-linter": "^0.2.8",
    "inline-style-prefix-all": "^1.0.1",
    "inline-style-prefixer": "^1.0.1",
    "inline-style-transformer": "^1.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.5.1",
    "mocha": "^2.4.5",
    "path": "^0.12.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-look-core": "^1.0.0-beta5",
    "react-native": "^0.20.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.5.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  }
}
